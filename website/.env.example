# Copy this file to .env.local for local development (do NOT commit .env.local)
# On Vercel: Project -> Settings -> Environment Variables

# Required for Vercel Blob uploads (create a Blob store in Vercel -> Storage)
BLOB_READ_WRITE_TOKEN=

# Optional: base path / asset prefix (leave empty for normal deployments)
# Example: /myapp
NEXT_PUBLIC_BASE_PATH=
